error 기록
Property 'value' does not exist on type 'EventTarget'.
const onChangeBodyInfo = (event: { target: HTMLInputElement }) => {
console.log(event.target.name);
};

const handleChange = (event: FormEvent<HTMLFormElement>) => {
console.log(event.target.name);
};
const handleChange = (event: React.ChangeEvent<HTMLFormElement>) => {
console.log(event.target.name);
};

BaseSyntheticEvent<Event, EventTarget & HTMLFormElement, EventTarget>.target: EventTarget
ChangeEvent<HTMLFormElement>.target: EventTarget & HTMLFormElement
